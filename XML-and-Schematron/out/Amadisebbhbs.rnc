  element front {
    ((amadis.model.frontPart
      | amadis.model.pLike
      | amadis.model.pLike.front
      | amadis.model.global)*,
     (((amadis.model.div1Like,
        (amadis.model.div1Like
         | amadis.model.frontPart
         | amadis.model.global)*)
       | (amadis.model.divLike,
          (amadis.model.divLike
           | amadis.model.frontPart
           | amadis.model.global)*)),
      (amadis.model.divBottom,
       (amadis.model.divBottom | amadis.model.global)*)?)?),
    amadis.att.global.attributes,
    amadis.att.declaring.attributes,
    empty
  }
amadis.back =
  
  ## (back matter) contains any appendixes, etc. following the main part of a text. [4.7. Back Matter 4. Default Text Structure]
  element back {
    ((amadis.model.frontPart
      | amadis.model.pLike.front
      | amadis.model.pLike
      | amadis.model.listLike
      | amadis.model.global)*,
     ((amadis.model.div1Like,
       (amadis.model.frontPart
        | amadis.model.div1Like
        | amadis.model.global)*)
      | (amadis.model.divLike,
         (amadis.model.frontPart
          | amadis.model.divLike
          | amadis.model.global)*))?,
     (amadis.model.divBottomPart,
      (amadis.model.divBottomPart | amadis.model.global)*)?),
    amadis.att.global.attributes,
    amadis.att.declaring.attributes,
    empty
  }
amadis.att.global.facs.attributes =
