amadis.shift =
  
  ## marks the point at which some paralinguistic feature of a series of utterances by any one speaker changes. [8.3.6. Shifts]
  element shift {
    empty
    >> sch:pattern [
         id = "Amadis-shift-shiftNew-constraint-14"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:shift"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@new"
             role = "warning"
             "              \x{a}" ~
             "The @new attribute should always be supplied; use the special value\x{a}" ~
             '"normal" to indicate that the feature concerned ceases to be\x{a}' ~
             "remarkable at this point."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    amadis.att.global.attributes,
    amadis.att.ascribed.attributes,
    
    ## a paralinguistic feature.
    attribute feature {
      
      ## speed of utterance.
      "tempo"
      | 
        ## loudness.
        "loud"
      | 
        ## pitch range.
        "pitch"
      | 
        ## tension or stress pattern.
        "tension"
      | 
        ## rhythmic qualities.
        "rhythm"
      | 
        ## voice quality.
        "voice"
    }?,
    
    ## specifies the new state of the paralinguistic feature specified.
    [ a:defaultValue = "normal" ]
    attribute new { amadis.data.enumerated }?,
    empty
  }
amadis.model.rdgLike = amadis.rdg
amadis.model.rdgPart =
  amadis.wit
  | amadis.witStart
  | amadis.witEnd
  | amadis.lacunaStart
  | amadis.lacunaEnd
amadis.att.rdgPart.attributes = amadis.att.rdgPart.attribute.wit
amadis.att.rdgPart.attribute.wit =
  
  ## (witness or witnesses) contains a space-delimited list of one or more sigla indicating the witnesses to this reading beginning or ending at this point.
  attribute wit {
    list { amadis.data.pointer+ }
  }?
amadis.att.witnessed.attributes = amadis.att.witnessed.attribute.wit
amadis.att.witnessed.attribute.wit =
  
  ## (witness or witnesses) contains a space-delimited list of one or more pointers indicating the witnesses which attest to a given reading.
  attribute wit {
    list { amadis.data.pointer+ }
  }?
amadis.att.textCritical.attributes =
  amadis.att.source.attributes,
  amadis.att.textCritical.attribute.type,
  amadis.att.textCritical.attribute.cause,
  amadis.att.textCritical.attribute.varSeq,
  amadis.att.textCritical.attribute.hand
amadis.att.textCritical.attribute.type =
  
  ## classifies the reading according to some useful typology.
  ## Sample values include: 1] substantive; 2] orthographic
  attribute type { amadis.data.enumerated }?
amadis.att.textCritical.attribute.cause =
  
  ## classifies the cause for the variant reading, according to any appropriate typology of possible origins.
  ## Sample values include: 1] homeoteleuton; 2] homeoarchy; 3] paleographicConfusion; 4] haplography; 5] dittography; 6] falseEmendation
  attribute cause { amadis.data.enumerated }?
amadis.att.textCritical.attribute.varSeq =
  
  ## (variant sequence) provides a number indicating the position of this reading in a sequence, when there is reason to presume a sequence to the variants. 
  attribute varSeq { amadis.data.count }?
amadis.att.textCritical.attribute.hand =
  
  ## indicates the hand responsible for a particular reading in the witness.
  attribute hand { amadis.data.pointer }?
amadis.app =
  
  ## (apparatus entry) contains one entry in a critical apparatus, with an optional lemma and usually one or more readings or notes on the relevant passage. [12.1.1. The Apparatus Entry]
  element app {
    (amadis.model.global*,
     (amadis.lem,
      amadis.model.global*,
      (amadis.wit, amadis.model.global*)?)?,
     ((amadis.model.rdgLike,
       amadis.model.global*,
       (amadis.wit, amadis.model.global*)?)
      | (amadis.rdgGrp,
         amadis.model.global*,
         (amadis.wit, amadis.model.global*)?))*),
    amadis.att.global.attributes,
    
    ## classifies the variation contained in this element according to some convenient typology.
    attribute type { amadis.data.enumerated }?,
    
    ## identifies the beginning of the lemma in the base text.
    attribute from { amadis.data.pointer }?,
    
    ## identifies the endpoint of the lemma in the base text.
    attribute to { amadis.data.pointer }?,
    
    ## (location) indicates the location of the variation, when the location-referenced method of apparatus markup is used.
    attribute loc {
      list { amadis.data.word+ }
    }?,
    empty
  }
amadis.listApp =
  
  ## (list of apparatus entries) contains a list of apparatus entries. [12.2. Linking the Apparatus to the Text]
  element listApp {
    (amadis.model.headLike*, (amadis.app | amadis.listApp)+),
    amadis.att.global.attributes,
    amadis.att.sortable.attributes,
    amadis.att.declarable.attributes,
    amadis.att.typed.attributes,
    empty
  }
amadis.lem =
  
  ## (lemma) contains the lemma, or base text, of a textual variation. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element lem {
    (text
     | amadis.model.gLike
     | amadis.model.phrase
     | amadis.model.inter
     | amadis.model.global
     | amadis.model.rdgPart)*,
    amadis.att.global.attributes,
    amadis.att.textCritical.attributes,
    amadis.att.witnessed.attributes,
    empty
  }
amadis.rdg =
  
  ## (reading) contains a single reading within a textual variation. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element rdg {
    (text
     | amadis.model.gLike
     | amadis.model.phrase
     | amadis.model.inter
     | amadis.model.global
     | amadis.model.rdgPart)*,
    amadis.att.global.attributes,
    amadis.att.textCritical.attributes,
    amadis.att.witnessed.attributes,
    empty
  }
amadis.rdgGrp =
  
  ## (reading group) within a textual variation, groups two or more readings perceived to have a genetic relationship or other affinity. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element rdgGrp {
    (((amadis.rdgGrp, amadis.wit?)
      | ((amadis.lem, amadis.wit?)?,
         (amadis.model.rdgLike, amadis.wit?))*)+)
    >> sch:pattern [
         id = "Amadis-rdgGrp-only1lem-constraint-15"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:rdgGrp"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "count(tei:lem) < 2"
             "Only one <lem> element may appear within a <rdgGrp>"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    amadis.att.global.attributes,
    amadis.att.textCritical.attributes,
    empty
  }
amadis.witDetail =
  
  ## (witness detail) gives further information about a particular witness, or witnesses, to a particular reading. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element witDetail {
    amadis.macro.phraseSeq,
    amadis.att.global.attributes,
    amadis.att.placement.attributes,
    amadis.att.source.attributes,
    amadis.att.pointing.attributes,
    
    ## (witnesses) indicates the sigil or sigla identifying the witness or witnesses to which the detail refers.
    attribute wit {
      list { amadis.data.pointer+ }
    },
    
    ## describes the type of information given about the witness.
    attribute type { amadis.data.enumerated }?,
    empty
  }
amadis.wit =
  
  ## contains a list of one or more sigla of witnesses attesting a given reading, in a textual variation. [12.1.4. Witness Information]
  element wit {
    amadis.macro.phraseSeq,
    amadis.att.global.attributes,
    amadis.att.rdgPart.attributes,
    empty
  }
amadis.listWit =
  
  ## (witness list) lists definitions for all the witnesses referred to by a critical apparatus, optionally grouped hierarchically. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element listWit {
    (amadis.model.headLike?, (amadis.witness | amadis.listWit)+),
    amadis.att.global.attributes,
    amadis.att.sortable.attributes,
    empty
  }
amadis.witness =
  
  ## contains either a description of a single witness referred to within the critical apparatus, or a list of witnesses which is to be referred to by a single sigil. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element witness {
    amadis.macro.limitedContent,
    amadis.att.global.attributes,
    amadis.att.sortable.attributes,
    empty
  }
amadis.witStart =
  
  ## (fragmented witness start) indicates the beginning, or resumption, of the text of a fragmentary witness. [12.1.5. Fragmentary Witnesses]
  element witStart {
    empty,
    amadis.att.global.attributes,
    amadis.att.rdgPart.attributes,
    empty
  }
amadis.witEnd =
  
  ## (fragmented witness end) indicates the end, or suspension, of the text of a fragmentary witness. [12.1.5. Fragmentary Witnesses]
  element witEnd {
    empty,
    amadis.att.global.attributes,
    amadis.att.rdgPart.attributes,
    empty
  }
amadis.lacunaStart =
  
  ## indicates the beginning of a lacuna in the text of a mostly complete textual witness. [12.1.5. Fragmentary Witnesses]
  element lacunaStart {
    empty,
    amadis.att.global.attributes,
    amadis.att.rdgPart.attributes,
    empty
  }
amadis.lacunaEnd =
  
  ## indicates the end of a lacuna in a mostly complete textual witness. [12.1.5. Fragmentary Witnesses]
  element lacunaEnd {
    empty,
    amadis.att.global.attributes,
    amadis.att.rdgPart.attributes,
    empty
  }
amadis.variantEncoding =
  
  ## declares the method used to encode text-critical variants. [12.1.1. The Apparatus Entry]
  element variantEncoding {
    empty,
    amadis.att.global.attributes,
    
    ## indicates which method is used to encode the apparatus of variants.
    attribute method {
      
      ## apparatus uses line numbers or other canonical reference scheme referenced in a base text.
      "location-referenced"
      | 
        ## apparatus indicates the precise locations of the beginning and ending of each lemma relative to a base text.
        "double-end-point"
      | 
        ## alternate readings of a passage are given in parallel in the text; no notion of a base text is necessary.
        "parallel-segmentation"
    },
    
    ## indicates whether the apparatus appears within the running text or external to it.
    attribute location {
      
      ## apparatus appears within the running text.
      "internal"
      | 
        ## apparatus appears outside the base text.
        "external"
    }
    >> sch:pattern [
         id =
           "Amadis-variantEncoding-location-variantEncodingLocation-constraint-11"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:variantEncoding"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "(@location != 'external') or (@method != 'parallel-segmentation')"
             "\x{a}" ~
             '              The @location value "external" is inconsistent with the\x{a}' ~
             "              parallel-segmentation method of apparatus markup."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    empty
  }
amadis.TEI =
  
  ## (TEI document) contains a single TEI-conformant document, containing a single TEI header, a single text, one or more members of the model.resourceLike class, or a combination of these. A series of TEI elements may be combined together to form a teiCorpus element. [4. Default Text Structure 15.1. Varieties of Composite Text]
  element TEI {
    (amadis.teiHeader,
     ((amadis.model.resourceLike+, amadis.text?) | amadis.text))
    >> sch:ns [ prefix = "tei" uri = "http://www.tei-c.org/ns/1.0" ]
    >> sch:ns [ prefix = "xs" uri = "http://www.w3.org/2001/XMLSchema" ]
    >> sch:ns [
         prefix = "rng"
         uri = "http://relaxng.org/ns/structure/1.0"
       ],
    amadis.att.global.attributes,
    
    ## specifies the major version number of the TEI Guidelines against which this document is valid.
    attribute version { amadis.data.version }?,
    empty
  }
amadis.text =
  
  ## contains a single text of any kind, whether unitary or composite, for example a poem or drama, a collection of essays, a novel, a dictionary, or a corpus sample. [4. Default Text Structure 15.1. Varieties of Composite Text]
  element text {
    (amadis.model.global*,
     (amadis.front, amadis.model.global*)?,
     (amadis.body | amadis.group),
     amadis.model.global*,
     (amadis.back, amadis.model.global*)?),
    amadis.att.global.attributes,
    amadis.att.declaring.attributes,
    amadis.att.typed.attributes,
    empty
  }
amadis.body =
  
  ## (text body) contains the whole body of a single unitary text, excluding any front or back matter. [4. Default Text Structure]
  element body {
    (amadis.model.global*,
     (amadis.model.divTop,
      (amadis.model.global | amadis.model.divTop)*)?,
     (amadis.model.divGenLike,
      (amadis.model.global | amadis.model.divGenLike)*)?,
     ((amadis.model.divLike,
       (amadis.model.global | amadis.model.divGenLike)*)+
      | (amadis.model.div1Like,
         (amadis.model.global | amadis.model.divGenLike)*)+
      | ((amadis.model.common, amadis.model.global*)+,
         ((amadis.model.divLike,
           (amadis.model.global | amadis.model.divGenLike)*)+
          | (amadis.model.div1Like,
             (amadis.model.global | amadis.model.divGenLike)*)+)?)),
     (amadis.model.divBottom, amadis.model.global*)*),
    amadis.att.global.attributes,
    amadis.att.declaring.attributes,
    empty
  }
amadis.group =
  
  ## contains the body of a composite text, grouping together a sequence of distinct texts (or groups of such texts) which are regarded as a unit for some purpose, for example the collected works of an author, a sequence of prose essays, etc. [4. Default Text Structure 4.3.1. Grouped Texts 15.1. Varieties of Composite Text]
  element group {
    ((amadis.model.divTop | amadis.model.global)*,
     ((amadis.text | amadis.group),
      (amadis.text | amadis.group | amadis.model.global)*),
     amadis.model.divBottom*),
    amadis.att.global.attributes,
    amadis.att.declaring.attributes,
    amadis.att.typed.attributes,
    empty
  }
amadis.floatingText =
  
  ## contains a single text of any kind, whether unitary or composite, which interrupts the text containing it at any point and after which the surrounding text resumes. [4.3.2. Floating Texts]
  element floatingText {
    (amadis.model.global*,
     (amadis.front, amadis.model.global*)?,
     (amadis.body | amadis.group),
     amadis.model.global*,
     (amadis.back, amadis.model.global*)?),
    amadis.att.global.attributes,
    amadis.att.declaring.attributes,
    amadis.att.typed.attributes,
    empty
  }
amadis.div =
  
  ## (text division) contains a subdivision of the front, body, or back of a text. [4.1. Divisions of the Body]
  element div {
    ((amadis.model.divTop | amadis.model.global)*,
     ((((amadis.model.divLike | amadis.model.divGenLike),
        amadis.model.global*)+
       | ((amadis.model.common, amadis.model.global*)+,
          ((amadis.model.divLike | amadis.model.divGenLike),
           amadis.model.global*)*)),
      (amadis.model.divBottom, amadis.model.global*)*)?),
    amadis.att.global.attribute.n,
    amadis.att.global.attribute.xmllang,
    amadis.att.global.attribute.xmlbase,
    amadis.att.global.attribute.xmlspace,
    amadis.att.global.rendition.attribute.rend,
    amadis.att.global.rendition.attribute.style,
    amadis.att.global.rendition.attribute.rendition,
    amadis.att.global.linking.attribute.corresp,
    amadis.att.global.linking.attribute.synch,
    amadis.att.global.linking.attribute.sameAs,
    amadis.att.global.linking.attribute.copyOf,
    amadis.att.global.linking.attribute.next,
    amadis.att.global.linking.attribute.prev,
    amadis.att.global.linking.attribute.exclude,
    amadis.att.global.linking.attribute.select,
    amadis.att.global.analytic.attribute.ana,
    amadis.att.global.facs.attribute.facs,
    amadis.att.global.change.attribute.change,
    amadis.att.global.responsibility.attribute.cert,
    amadis.att.global.responsibility.attribute.resp,
    amadis.att.divLike.attributes,
    amadis.att.typed.attributes,
    amadis.att.declaring.attributes,
    
    ## (identifier) provides a unique identifier for the element bearing the attribute.
    attribute xml:id { text },
    empty
  }
amadis.trailer =
  
  ## contains a closing title or footer appearing at the end of a division of a text. [4.2.4. Content of Textual Divisions 4.2. Elements Common to All Divisions]
  element trailer {
    (text
     | amadis.lg
     | amadis.model.gLike
     | amadis.model.phrase
     | amadis.model.inter
     | amadis.model.lLike
     | amadis.model.global)*,
    amadis.att.global.attributes,
    amadis.att.typed.attributes,
    empty
  }
amadis.byline =
  
  ## contains the primary statement of responsibility given for a work on its title page or at the head or end of the work. [4.2.2. Openers and Closers 4.5. Front Matter]
  element byline {
    (text
     | amadis.model.gLike
     | amadis.model.phrase
     | amadis.docAuthor
     | amadis.model.global)*,
    amadis.att.global.attributes,
    empty
  }
amadis.dateline =
  
  ## contains a brief description of the place, date, time, etc. of production of a letter, newspaper story, or other work, prefixed or suffixed to it as a kind of heading or trailer. [4.2.2. Openers and Closers]
  element dateline {
    (text
     | amadis.model.gLike
     | amadis.model.phrase
     | amadis.model.global
     | amadis.docDate)*,
    amadis.att.global.attributes,
    empty
  }
amadis.argument =
  
  ## contains a formal list or prose description of the topics addressed by a subdivision of a text. [4.2. Elements Common to All Divisions 4.6. Title Pages]
  element argument {
    ((amadis.model.global | amadis.model.headLike)*,
     (amadis.model.common, amadis.model.global*)+),
    amadis.att.global.attributes,
    empty
  }
amadis.epigraph =
  
  ## contains a quotation, anonymous or attributed, appearing at the start or end of a section or on a title page. [4.2.3. Arguments, Epigraphs, and Postscripts 4.2. Elements Common to All Divisions 4.6. Title Pages]
  element epigraph {
    (amadis.model.common | amadis.model.global)*,
    amadis.att.global.attributes,
    empty
  }
amadis.opener =
  
  ## groups together dateline, byline, salutation, and similar phrases appearing as a preliminary group at the start of a division, especially of a letter. [4.2. Elements Common to All Divisions]
  element opener {
    (text
     | amadis.model.gLike
     | amadis.model.phrase
     | amadis.argument
     | amadis.byline
     | amadis.dateline
     | amadis.epigraph
     | amadis.salute
     | amadis.signed
     | amadis.model.global)*,
    amadis.att.global.attributes,
    empty
  }
amadis.closer =
  
  ## groups together salutations, datelines, and similar phrases appearing as a final group at the end of a division, especially of a letter. [4.2.2. Openers and Closers 4.2. Elements Common to All Divisions]
  element closer {
    (text
     | amadis.model.gLike
     | amadis.signed
     | amadis.dateline
     | amadis.salute
     | amadis.model.phrase
     | amadis.model.global)*,
    amadis.att.global.attributes,
    empty
  }
amadis.salute =
  
  ## (salutation) contains a salutation or greeting prefixed to a foreword, dedicatory epistle, or other division of a text, or the salutation in the closing of a letter, preface, etc. [4.2.2. Openers and Closers]
  element salute {
    amadis.macro.paraContent, amadis.att.global.attributes, empty
  }
amadis.signed =
  
  ## (signature) contains the closing salutation, etc., appended to a foreword, dedicatory epistle, or other division of a text. [4.2.2. Openers and Closers]
  element signed {
    amadis.macro.paraContent, amadis.att.global.attributes, empty
  }
amadis.postscript =
  
  ## contains a postscript, e.g. to a letter. [4.2. Elements Common to All Divisions]
  element postscript {
    ((amadis.model.global | amadis.model.divTopPart)*,
     amadis.model.common,
     (amadis.model.global | amadis.model.common)*,
     (amadis.model.divBottomPart, amadis.model.global*)*),
    amadis.att.global.attributes,
    empty
  }
amadis.titlePage =
  
  ## (title page) contains the title page of a text, appearing within the front or back matter. [4.6. Title Pages]
  element titlePage {
    (amadis.model.global*,
     amadis.model.titlepagePart,
     (amadis.model.titlepagePart | amadis.model.global)*),
    amadis.att.global.attributes,
    
    ## classifies the title page according to any convenient typology.
    attribute type { amadis.data.enumerated }?,
    empty
  }
amadis.docTitle =
  
  ## (document title) contains the title of a document, including all its constituents, as given on a title page. [4.6. Title Pages]
  element docTitle {
    (amadis.model.global*, (amadis.titlePart, amadis.model.global*)+),
    amadis.att.global.attributes,
    amadis.att.canonical.attributes,
    empty
  }
amadis.titlePart =
  
  ## contains a subsection or division of the title of a work, as indicated on a title page. [4.6. Title Pages]
  element titlePart {
    amadis.macro.paraContent,
    amadis.att.global.attributes,
    
    ## specifies the role of this subdivision of the title.
    ## Suggested values include: 1] main; 2] sub(subordinate) ; 3] alt(alternate) ; 4] short; 5] desc(descriptive) 
    [ a:defaultValue = "main" ]
    attribute type {
      
      ## main title of the work
      "main"
      | 
        ## (subordinate) subtitle of the work
        "sub"
      | 
        ## (alternate) alternative title of the work
        "alt"
      | 
        ## abbreviated form of title
        "short"
      | 
        ## (descriptive) descriptive paraphrase of the work
        "desc"
      | xsd:Name
    }?,
    empty
  }
amadis.docAuthor =
  
  ## (document author) contains the name of the author of the document, as given on the title page (often but not always contained in a byline). [4.6. Title Pages]
  element docAuthor {
    amadis.macro.phraseSeq,
    amadis.att.global.attributes,
    amadis.att.canonical.attributes,
    empty
  }
amadis.imprimatur =
  
  ## contains a formal statement authorizing the publication of a work, sometimes required to appear on a title page or its verso. [4.6. Title Pages]
  element imprimatur {
    amadis.macro.paraContent, amadis.att.global.attributes, empty
  }
amadis.docEdition =
  
  ## (document edition) contains an edition statement as presented on a title page of a document. [4.6. Title Pages]
  element docEdition {
    amadis.macro.paraContent, amadis.att.global.attributes, empty
  }
amadis.docImprint =
  
  ## (document imprint) contains the imprint statement (place and date of publication, publisher name), as given (usually) at the foot of a title page. [4.6. Title Pages]
  element docImprint {
    (text
     | amadis.model.gLike
     | amadis.model.phrase
     | amadis.pubPlace
     | amadis.docDate
     | amadis.publisher
     | amadis.model.global)*,
    amadis.att.global.attributes,
    empty
  }
amadis.docDate =
  
  ## (document date) contains the date of a document, as given on a title page or in a dateline. [4.6. Title Pages]
  element docDate {
    amadis.macro.phraseSeq,
    amadis.att.global.attributes,
    
    ## gives the value of the date in standard form, i.e. YYYY-MM-DD.
    attribute when { amadis.data.temporal.w3c }?,
    empty
  }
amadis.front =
  
