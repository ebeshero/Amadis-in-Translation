    ((text
      | amadis.model.gLike
      | amadis.model.global
      | amadis.binaryObject
      | amadis.formula
      | amadis.graphic
      | amadis.media
      | amadis.distinct
      | amadis.emph
      | amadis.foreign
      | amadis.gloss
      | amadis.mentioned
      | amadis.soCalled
      | amadis.term
      | amadis.title
      | amadis.hi
      | amadis.caesura
      | amadis.rhyme
      | amadis.address
      | amadis.affiliation
      | amadis.email
      | amadis.date
      | amadis.time
      | amadis.depth
      | amadis.dim
      | amadis.geo
      | amadis.height
      | amadis.measure
      | amadis.measureGrp
      | amadis.num
      | amadis.width
      | amadis.name
      | amadis.orgName
      | amadis.persName
      | amadis.geogFeat
      | amadis.offset
      | amadis.addName
      | amadis.forename
      | amadis.genName
      | amadis.nameLink
      | amadis.roleName
      | amadis.surname
      | amadis.bloc
      | amadis.country
      | amadis.district
      | amadis.geogName
      | amadis.placeName
      | amadis.region
      | amadis.settlement
      | amadis.climate
      | amadis.location
      | amadis.population
      | amadis.state
      | amadis.terrain
      | amadis.trait
      | amadis.idno
      | amadis.rs
      | amadis.abbr
      | amadis.am
      | amadis.choice
      | amadis.ex
      | amadis.expan
      | amadis.subst
      | amadis.add
      | amadis.app
      | amadis.corr
      | amadis.damage
      | amadis.del
      | amadis.handShift
      | amadis.mod
      | amadis.orig
      | amadis.redo
      | amadis.reg
      | amadis.restore
      | amadis.retrace
      | amadis.sic
      | amadis.supplied
      | amadis.surplus
      | amadis.unclear
      | amadis.undo
      | amadis.catchwords
      | amadis.dimensions
      | amadis.heraldry
      | amadis.locus
      | amadis.locusGrp
      | amadis.material
      | amadis.objectType
      | amadis.origDate
      | amadis.origPlace
      | amadis.secFol
      | amadis.signatures
