           context = "tei:addSpan"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@spanTo"
             "The @spanTo attribute of "
             sch:name [ ]
             " is required."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "Amadis-addSpan-spanTo_fr-constraint-18"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:addSpan"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@spanTo"
             "L'attribut spanTo est requis."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    amadis.att.global.attributes,
    amadis.att.transcriptional.attributes,
    amadis.att.placement.attributes,
    amadis.att.typed.attributes,
    amadis.att.spanning.attributes,
    empty
  }
amadis.damage =
  
