    amadis.att.global.linking.attribute.prev,
    amadis.att.global.linking.attribute.exclude,
    amadis.att.global.linking.attribute.select,
    amadis.att.global.analytic.attribute.ana,
    amadis.att.global.facs.attribute.facs,
    amadis.att.global.change.attribute.change,
    amadis.att.global.responsibility.attribute.cert,
    amadis.att.typed.attributes,
    amadis.att.dimensions.attributes,
    amadis.att.source.attributes,
    
    ## (responsible party) (responsible party) indicates the individual responsible for identifying and measuring the space
    attribute resp {
      list { amadis.data.pointer+ }
    }?,
    
    ## (dimension) indicates whether the space is horizontal or vertical.
    attribute dim {
      
      ## the space is horizontal.
      "horizontal"
      | 
        ## the space is vertical.
        "vertical"
    }?,
    empty
  }
amadis.subst =
  
  ## (substitution) groups one or more deletions with one or more additions when the combination is to be regarded as a single intervention in the text. [11.3.1.5. Substitutions]
  element subst {
    ((amadis.add | amadis.del | amadis.model.milestoneLike)+)
    >> sch:pattern [
         id = "Amadis-subst-substContents1-constraint-23"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:subst"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "child::tei:add and child::tei:del"
             "\x{a}" ~
             "                  "
             sch:name [ ]
             " must have at least one child add and at least one child del"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    amadis.att.global.attributes,
    amadis.att.transcriptional.attributes,
    empty
  }
amadis.substJoin =
  
  ## (substitution join) identifies a series of possibly fragmented additions, deletions or other revisions on a manuscript that combine to make up a single intervention in the text [11.3.1.5. Substitutions]
