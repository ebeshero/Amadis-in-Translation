    amadis.macro.phraseSeq
    >> sch:let [ name = "si" value = "doc('SI-Amadis.xml')//@xml:id" ]
    >> sch:let [ name = "siFile" value = "doc('SI-Amadis.xml')" ]
    >> sch:pattern [
         id = "Amadis-persName-schnizz-constraint-10"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "@ref | @resp | @corresp"
           "\x{a}" ~
           "					          "
           sch:let [
             name = "tokens"
             value =
               "for $i in tokenize(., '\s+') return substring-after($i,'#')"
           ]
           "\x{a}" ~
           "					          "
           sch:assert [
             test = "every $token in $tokens satisfies $token = $si"
             "The attribute (after the hashtag, #) must match a defined @xml:id in the Site Index file!"
           ]
           "\x{a}" ~
           "				        "
         ]
         "\x{a}" ~
         "         "
       ],
    amadis.att.global.attribute.xmllang,
    amadis.att.global.rendition.attribute.rend,
    amadis.att.global.change.attribute.change,
    amadis.att.global.responsibility.attribute.cert,
    amadis.att.naming.attribute.role,
    amadis.att.canonical.attribute.ref,
    amadis.att.typed.attribute.type,
    empty
  }
