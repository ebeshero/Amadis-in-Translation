                  <zeroOrMore>
                     <choice>
                        <ref name="amadis.TEI"/>
                        <ref name="amadis.teiCorpus"/>
                     </choice>
                  </zeroOrMore>
               </group>
               <oneOrMore>
                  <choice>
                     <ref name="amadis.TEI"/>
                     <ref name="amadis.teiCorpus"/>
