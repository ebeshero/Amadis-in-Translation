         <ref name="amadis.att.global.change.attribute.change"/>
         <ref name="amadis.att.global.responsibility.attribute.cert"/>
         <ref name="amadis.att.global.responsibility.attribute.resp"/>
         <ref name="amadis.att.segLike.attribute.function"/>
