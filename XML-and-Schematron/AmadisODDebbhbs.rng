            </group>
            <zeroOrMore>
               <group>
                  <group>
                     <ref name="amadis.model.divBottom"/>
                  </group>
                  <zeroOrMore>
                     <ref name="amadis.model.global"/>
                  </zeroOrMore>
               </group>
            </zeroOrMore>
         </group>
         <ref name="amadis.att.global.attributes"/>
         <ref name="amadis.att.declaring.attributes"/>
